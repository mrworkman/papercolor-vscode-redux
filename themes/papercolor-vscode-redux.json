          "meta.import.java keyword.other.import.java",
          "meta.import.java storage.modifier.java",
          "storage.type.primitive.java", // bold
          "keyword.control.try.java", // bold
          "keyword.control.catch.java", // bold
          "keyword.control.finally.java", // bold
          "meta.import.kotlin keyword.other.package.kotlin",
          "meta.import.kotlin keyword.other.import.kotlin",
          "meta.import.kotlin storage.modifier.kotlin",
          "meta.class.kotlin meta.block.kotlin entity.name.function.constructor", // bold
          "meta.import.groovy keyword.other.import.groovy",
          "meta.import.groovy keyword.other.import.static.groovy",
          "storage.type.primitive.groovy", // bold
          "storage.type.def.groovy", // bold
          "keyword.control.flow.return.cs", // bold
          "keyword.control.flow.throw.cs", // bold
