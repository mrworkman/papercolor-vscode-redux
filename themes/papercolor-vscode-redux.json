{
   "$schema": "vscode://schemas/color-theme",
   "name": "Papercolor Redux",
   "type": "light",

   "colors": {

      // activityBar
      "activityBar.background": "#eeeeee",
      "activityBar.border": "#B0BEC5",
      "activityBar.dropBackground": "#155f87",
      "activityBar.foreground": "#155f87",
      "activityBarBadge.background": "#d70087",
      "activityBarBadge.foreground": "#eeeeee",

      // badge
      "badge.background": "#d70087",
      "badge.foreground": "#eeeeee",

      // button
      "button.background": "#0277BD",
      "button.foreground": "#eeeeee",
      "button.hoverBackground": "#155f87",

      // contrast
      "contrastActiveBorder": "#00000000",
      "contrastBorder": "#ffffff00",

      // debug
      "debugExceptionWidget.background": "#eeeeee",
      "debugExceptionWidget.border": "#0d3a58",
      "debugToolBar.background": "#eeeeee",

      // description
      "descriptionForeground": "#aaa",

      // diff
      "diffEditor.insertedTextBackground": "#00C85320",
      "diffEditor.insertedTextBorder": "#00C85380",
      "diffEditor.removedTextBackground": "#F5005720",
      "diffEditor.removedTextBorder": "#F5005780",

      // dropdown
      "dropdown.background": "#ffffff",
      "dropdown.border": "#eeeeee",
      "dropdown.foreground": "#424242",

      // editor
      // This is the main background color
      "editor.background": "#eeeeee",
      // this is the main text colour
      "editor.foreground": "#424242",

      // Currently found item
      "editor.findMatchBackground": "#ffff8d",
      // Other Found Items in the document
      "editor.findMatchHighlightBackground": "#FFFDE7aa",
      "editor.findMatchHighlightBorder": "#FFFDE7",

      // Highlights the "range" (lines) in the selection that the search applies to.
      "editor.findRangeHighlightBackground": "#CFD8DC60",

      // When you hover over something and a popup shows, this highlights that thing
      "editor.hoverHighlightBackground": "#B0BEC5dd",

      // current line styles
      "editor.lineHighlightBackground": "#B3E5FCe8",
      "editor.lineHighlightBorder": "#81D4FA",
      "editor.rangeHighlightBackground": "#B3E5FC",

      // selected Text colours
      // This is the standard Select colour.
      "editor.selectionBackground": "#81D4FA",
      "editor.inactiveSelectionBackground": "#81D4FA",

      // This is the colour of the other matching elements
      "editor.selectionHighlightBackground": "#B3E5FC",

      // Word Highlights! This happens when you move your cursor inside a variable
      // Strong is the one where your cursor currently is
      "editor.wordHighlightStrongBackground": "#ffffff21",
      // and this one is the rest of them
      "editor.wordHighlightBackground": "#ffffff21",
      "editorBracketMatch.background": "#e0e0e0",
      "editorBracketMatch.border": "#9e9e9e",
      "editorCodeLens.foreground": "#aaa",

      "editorCursor.foreground": "#757575",
      "editorError.border": "#00000000",
      "editorError.foreground": "#F50057",

      // gutter
      "editorGutter.background": "#eeeeee",
      "editorGutter.addedBackground": "#3C9F4A",
      "editorGutter.deletedBackground": "#A22929",
      "editorGutter.modifiedBackground": "#26506D",

      // editorGroup
      "editorGroup.border": "#CFD8DC",
      "editorGroup.dropBackground": "#12273899",

      // editorGroupHeader
      "editorGroupHeader.noTabsBackground": "#eeeeee",
      "editorGroupHeader.tabsBackground": "#eeeeee",
      "editorGroupHeader.tabsBorder": "#B0BEC5",

      // editorHoverWidget
      "editorHoverWidget.background": "#eeeeee",
      "editorHoverWidget.border": "#546E7A",

      // Vertical indentation guides (lines)
      "editorIndentGuide.background": "#155f8766",
      "editorIndentGuide.activeBackground": "#155f87a8",

      // Line numbers displayed in the gutter.
      "editorLineNumber.foreground": "#bdbdbd",
      "editorLineNumber.activeForeground": "#78909C",

      "editorLink.activeForeground": "#aaa",
      // editorMarkerNavigation
      "editorMarkerNavigation.background": "#3B536433",
      "editorMarkerNavigationError.background": "#A22929",
      "editorMarkerNavigationWarning.background": "#ffc600",
      // ruler
      "editorOverviewRuler.border": "#0d3a58",
      "editorOverviewRuler.commonContentForeground": "#ffc60055",
      "editorOverviewRuler.currentContentForeground": "#ee3a4355",
      "editorOverviewRuler.incomingContentForeground": "#3ad90055",
      "editorRuler.foreground": "#1F4662",
      // editorSuggestWidget
      "editorSuggestWidget.background": "#eeeeee",
      "editorSuggestWidget.border": "#0d3a58",
      "editorSuggestWidget.foreground": "#424242",
      "editorSuggestWidget.highlightForeground": "#d70087",
      "editorSuggestWidget.selectedBackground": "#B3E5FCe8",
      // editorWarning
      "editorWarning.border": "#ffffff00",
      "editorWarning.foreground": "#ffc600",
      "editorWhitespace.foreground": "#0D47A148",

      "editorWidget.background": "#eeeeee",
      "editorWidget.resizeBorder": "#155f87",
      "editorWidget.border": "#0d3a58",

      "errorForeground": "#FF1744",
      // extensionButton
      "extensionButton.prominentBackground": "#0088ff",
      "extensionButton.prominentForeground": "#fff",
      "extensionButton.prominentHoverBackground": "#ff9d00",
      "focusBorder": "#0d3a58",

      "foreground": "#424242",

      // input
      "input.background": "#ffffff",
      "input.border": "#eeeeee",
      "input.foreground": "#424242",
      "input.placeholderForeground": "#9E9E9E",
      "inputOption.activeBorder": "#d70087",

      "inputValidation.errorBackground": "#FCE4EC",
      "inputValidation.errorBorder": "#EC407A",
      "inputValidation.infoBackground": "#193549",
      "inputValidation.infoBorder": "#0D3A58",
      "inputValidation.warningBackground": "#193549",
      "inputValidation.warningBorder": "#ffc600",

      // list
      "list.activeSelectionBackground": "#0091EA",
      "list.activeSelectionForeground": "#eeeeee",
      "list.dropBackground": "#81D4FA",
      "list.errorForeground": "#D50000",
      "list.focusBackground": "#B3E5FC",
      "list.focusForeground": "#424242",
      "list.highlightForeground": "#d70087",
      "list.hoverBackground": "#B3E5FC",
      "list.hoverForeground": "#424242",
      "list.inactiveFocusBackground": "#4FC3F7",
      "list.inactiveSelectionBackground": "#0091EA88",
      "list.inactiveSelectionForeground": "#424242",
      "list.warningForeground": "#FF6F00",

      // menu
      "menu.background": "#eeeeee",
      "menu.foreground": "#444444",
      "menu.selectionBackground": "#0087af",
      "menu.selectionForeground": "#eeeeee",

      "menubar.selectionBackground": "#0087af",
      "menubar.selectionForeground": "#eeeeee",

      // merge
      "merge.border": "#ffffff00",
      "merge.commonContentBackground": "#c97d0c",
      "merge.commonHeaderBackground": "#c97d0c",
      "merge.currentContentBackground": "#2F7366",
      "merge.currentHeaderBackground": "#2F7366",
      "merge.incomingContentBackground": "#185294",
      "merge.incomingHeaderBackground": "#185294",

      // notification colors - The colors below only apply for VS Code versions 1.21 and higher.
      "notificationCenter.border": "#0d3a58",
      "notificationCenterHeader.foreground": "#424242",
      "notificationCenterHeader.background": "#eeeeee",
      "notificationToast.border": "#0d3a58",
      "notifications.foreground": "#424242",
      "notifications.background": "#eeeeee",
      "notifications.border": "#0d3a58",
      "notificationLink.foreground": "#424242",

      // panel (e.g. terminal "header")
      "panel.background": "#eeeeee",
      "panel.border": "#B0BEC5",
      "panelTitle.activeBorder": "#4A148C",
      "panelTitle.activeForeground": "#424242",
      "panelTitle.inactiveForeground": "#757575",

      // "peekView
      "peekView.border": "#B0BEC5",
      "peekViewEditor.background": "#ECEFF1",
      "peekViewEditor.matchHighlightBackground": "#19354900",
      "peekViewEditorGutter.background": "#E0E0E0",
      "peekViewResult.background": "#ECEFF1",
      "peekViewResult.fileForeground": "#f00",
      "peekViewResult.lineForeground": "#424242",
      "peekViewResult.matchHighlightBackground": "#ffff8d",
      "peekViewResult.selectionBackground": "#B3E5FC",
      "peekViewResult.selectionForeground": "#424242",
      "peekViewTitle.background": "#CFD8DC",
      "peekViewTitleDescription.foreground": "#263238",
      "peekViewTitleLabel.foreground": "#d70087",

      // picker
      "pickerGroup.border": "#0d3a58",
      "pickerGroup.foreground": "#aaa",
      // progressBar
      "progressBar.background": "#d70087",

      // scrollbar
      "scrollbar.shadow": "#00000000",
      "scrollbarSlider.activeBackground": "#58585880",
      "scrollbarSlider.background": "#58585820",
      "scrollbarSlider.hoverBackground": "#58585840",

      // selection
      "selection.background": "#B3E5FC",

      "settings.checkboxBackground": "#ffffff",
      "settings.checkboxForeground": "#d70087",
      "settings.headerForeground": "#155f87",

      // sidebar
      "sideBar.background": "#eeeeee",
      "sideBar.border": "#B0BEC5",
      "sideBar.foreground": "#424242",
      "sideBarSectionHeader.background": "#E0E0E0",
      "sideBarSectionHeader.foreground": "#424242",
      "sideBarTitle.foreground": "#424242",

      // statusBar
      "statusBar.background": "#155f87",
      "statusBar.border": "#4A148C",
      "statusBar.debuggingBackground": "#D50000",
      "statusBar.debuggingBorder": "#4A148C",
      "statusBar.debuggingForeground": "#eeeeee",
      "statusBar.foreground": "#eeeeee",
      "statusBar.noFolderBackground": "#155f87",
      "statusBar.noFolderBorder": "#4A148C",
      "statusBar.noFolderForeground": "#eeeeee",

      "statusBarItem.activeBackground": "#039BE5",
      "statusBarItem.hoverBackground": "#0277BD",
      "statusBarItem.prominentBackground": "#039BE5",
      "statusBarItem.prominentHoverBackground": "#0277BD",

      // tabs.
      "tab.activeBackground": "#eeeeee",
      "tab.activeForeground": "#424242",
      "tab.border": "#B0BEC5",
      "tab.activeBorder": "#eeeeee",
      "tab.inactiveBackground": "#E0E0E0",
      "tab.inactiveForeground": "#757575",
      "tab.unfocusedActiveForeground": "#757575",
      "tab.unfocusedInactiveForeground": "#9E9E9E",

      // --- workbench: terminal
      "terminal.ansiBlack": "#000000",
      "terminal.ansiRed": "#CC0000",
      "terminal.ansiGreen": "#508A18",
      "terminal.ansiYellow": "#EA780C",
      "terminal.ansiBlue": "#005F87",
      "terminal.ansiMagenta": "#75507B",
      "terminal.ansiCyan": "#008787",
      "terminal.ansiWhite": "#ffffff",
      "terminal.ansiBrightBlack": "#738287",
      "terminal.ansiBrightRed": "#EF2929",
      "terminal.ansiBrightGreen": "#6DB525",
      "terminal.ansiBrightYellow": "#FF8412",
      "terminal.ansiBrightBlue": "#0087AF",
      "terminal.ansiBrightMagenta": "#AD7FA8",
      "terminal.ansiBrightCyan": "#00B0B0",
      "terminal.ansiBrightWhite": "#193549",
      "terminal.background": "#CFD8DC",
      "terminal.foreground": "#444444",
      "terminal.selectionBackground": "#155f8788",
      "terminalCursor.background": "#ddd",
      "terminalCursor.foreground": "#757575",

      // Git status colors in File Explorer
      "gitDecoration.modifiedResourceForeground": "#F57F17",
      "gitDecoration.deletedResourceForeground": "#FF1744",
      "gitDecoration.untrackedResourceForeground": "#33691E",
      "gitDecoration.ignoredResourceForeground": "#616161",
      "gitDecoration.conflictingResourceForeground": "#FF3D00",

      // textBlockQuote
      "textBlockQuote.background": "#EDE7F6",
      "textBlockQuote.border": "#6200EA",
      "textCodeBlock.background": "#E0E0E0",

      "textLink.activeForeground": "#7E57C2",
      "textLink.foreground": "#311B92",

      "textPreformat.foreground": "#d70087",
      "textSeparator.foreground": "#f00",

      "titleBar.border": "#4A148C",
      "titleBar.activeBackground": "#01579B",
      "titleBar.activeForeground": "#eeeeee",
      "titleBar.inactiveBackground": "#01579B",
      "titleBar.inactiveForeground": "#eeeeeeaa",

      "walkThrough.embeddedEditorBackground": "#e0e0e068",
      "welcomePage.buttonBackground": "#CFD8DC",
      "welcomePage.buttonHoverBackground": "#B0BEC5",
      "welcomePage.background": "#ECEFF1",
      "widget.shadow": "#155f8766"
    },

    // Get scopes with:
    // > Developer: Inspect TM Scopes

    "tokenColors": [
      {
        "name": "Comment",
        "scope": ["comment", "punctuation.definition.comment"],
        "settings": {
          "fontStyle": "italic",
          "foreground": "#878787"
        }
      },
      {
        "name": "Constant",
        "scope": "constant",
        "settings": {
          "foreground": "#d86307"
        }
      },
      {
        "name": "Entity",
        "scope": "entity",
        "settings": {
          "foreground": "#444444"
        }
      },
      {
        "name": "Invalid",
        "scope": "invalid",
        "settings": {
          "foreground": "#af0000",
        }
      },
      {
        "name": "Storage Type Function",
        "scope": "storage.type.function",
        "settings": {
          "foreground": "#d70087",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Keyword",
        "scope": "keyword, storage.type.class",
        "settings": {
          "foreground": "#d70087",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Meta",
        "scope": "meta",
        "settings": {
          "foreground": "#d70087"
        }
      },
      {
        "name": "Meta Brace",
        "scope": "meta.brace",
        "settings": {
          "foreground": "#424242"
        }
      },
      {
        "name": "Punctuation",
        "scope": "punctuation",
        "settings": {
          "foreground": "#424242"
        }
      },
      {
        "name": "Punctuation Parameters",
        "scope": "punctuation.definition.parameters",
        "settings": {
          "foreground": "#424242"
        }
      },
      {
        "name": "Punctuation Template Expression",
        "scope": "punctuation.definition.template-expression",
        "settings": {
          "foreground": "#424242",
        }
      },
      {
        "name": "Storage",
        "scope": "storage",
        "settings": {
          "foreground": "#005f87",
          "fontStyle": "bold"
        }
      },
      {
        "name": "Storage Type Arrow Function",
        "scope": "storage.type.function.arrow",
        "settings": {
          "foreground": "#005f87"
        }
      },
      {
        "name": "String",
        "scope": ["string", "punctuation.definition.string"],
        "settings": {
          "foreground": "#6f9700",
          "fontStyle": ""
        }
      },
      {
        "name": "String Template",
        "scope": ["string.template", "punctuation.definition.string.template"],
        "settings": {
          "foreground": "#6f9700"
        }
      },
      {
        "name": "Support",
        "scope": "support",
        "settings": {
          "foreground": "#005f87"
        }
      },
      {
        "name": "Support Function",
        "scope": "support.function",
        "settings": {
          "foreground": "#AA00FF"
        }
      },
      {
        "name": "Support Variable Property DOM",
        "scope": "support.variable.property.dom",
        "settings": {
          "foreground": "#e1efff"
        }
      },
      {
        "name": "Variable",
        "scope": "variable",
        "settings": {
          "foreground": "#424242"
        }
      },
      {
        "name": "[CSS] - Entity",
        "scope": ["source.css entity", "source.stylus entity"],
        "settings": {
          "foreground": "#d70087"
        }
      },
      {
        "name": "[CSS] - ID Selector",
        "scope": "entity.other.attribute-name.id.css",
        "settings": {
          "foreground": "#d70087",
        }
      },
      {
        "name": "[CSS / YAML / WHAT ELSE??] - Element Selector",
        "scope": "entity.name.tag",
        "settings": {
          "foreground": "#005f87",
          "fontStyle": "bold"
        }
      },
      {
        "name": "[CSS] - Support",
        "scope": ["source.css support", "source.stylus support"],
        "settings": {
          "foreground": "#005f87",
          "fontStyle": "bold",
        }
      },
      {
        "name": "[CSS] - Constant",
        "scope": [
          "source.css constant",
          "source.css support.constant",
          "source.stylus constant",
          "source.stylus support.constant"
        ],
        "settings": {
          "foreground": "#d86307",
          "fontStyle": "bold"
        }
      },
      {
        "name": "[CSS] - Variable",
        "scope": ["source.css variable", "source.stylus variable"],
        "settings": {
          "foreground": "#424242"
        }
      },
      {
        "name":
          "Quotes.",
        "scope":
          "punctuation.definition.string.begin, punctuation.definition.string.end",
        "settings": {
          "foreground": "#6f9700"
        }
      },
      {
        "name": "[INI] - Entity",
        "scope": "source.ini entity",
        "settings": {
          "foreground": "#d70087"
        }
      },
      {
        "name": "[INI] - Keyword",
        "scope": "source.ini keyword",
        "settings": {
          "foreground": "#01579B"
        }
      },
      {
        "name": "[INI] - Punctuation Definition",
        "scope": "source.ini punctuation.definition",
        "settings": {
          "foreground": "#424242"
        }
      },
      {
        "name": "[INI] - Punctuation Separator",
        "scope": "source.ini punctuation.separator",
        "settings": {
          "foreground": "#424242"
        }
      },
      // {
      //   "name": "[JAVASCRIPT] - Variable Language",
      //   "scope": "variable.language, entity.name.type.class.js",
      //   "settings": {
      //     "foreground": "#AA00FF"
      //   }
      // },
      {
        "name": "[JAVASCRIPT] - Inherited Component",
        "scope": "entity.other.inherited-class.js",
        "settings": {
          "foreground": "#880E4F"
        }
      },
      {
        "name": "[MARKDOWN] - Heading Punctuation",
        "scope": "punctuation.definition.heading.markdown",
        "settings": {
          "foreground": "#424242"
        }
      },
      {
        "name": "[MARKDOWN] - Heading Name Section",
        "scope": [
          "entity.name.section.markdown",
          "markup.heading.setext.1.markdown",
          "markup.heading.setext.2.markdown"
        ],
        "settings": {
          "foreground": "#d70087",
          "fontStyle": "bold"
        }
      },
      {
        "name": "[MARKDOWN] - Paragraph",
        "scope": "meta.paragraph.markdown",
        "settings": {
          "foreground": "#424242"
        }
      },
      {
        "name": "[MARKDOWN] - Quote Punctuation",
        "scope": "beginning.punctuation.definition.quote.markdown",
        "settings": {
          "foreground": "#f00"
        }
      },
      {
        "name": "[MARKDOWN] - Quote Paragraph",
        "scope": "markup.quote.markdown meta.paragraph.markdown",
        "settings": {
          "fontStyle": "italic",
          "foreground": "#424242"
        }
      },
      {
        "name": "[MARKDOWN] - Separator",
        "scope": "meta.separator.markdown",
        "settings": {
          "foreground": "#f00"
        }
      },
      {
        "name": "[MARKDOWN] - Emphasis Bold",
        "scope": "markup.bold.markdown",
        "settings": {
          "foreground": "#424242",
          "fontStyle": "bold"
        }
      },
      {
        "name": "[MARKDOWN] - Emphasis Italic",
        "scope": "markup.italic.markdown",
        "settings": {
          "foreground": "#424242",
          "fontStyle": "italic"
        }
      },
      {
        "name": "[MARKDOWN] - Lists",
        "scope": "beginning.punctuation.definition.list.markdown",
        "settings": {
          "foreground": "#f00"
        }
      },
      {
        "name": "[MARKDOWN] - Link Title",
        "scope": "string.other.link.title.markdown",
        "settings": {
          "foreground": "#a5ff90"
        }
      },
      {
        "name": "[MARKDOWN] - Link/Image Title",
        "scope": [
          "string.other.link.title.markdown",
          "string.other.link.description.markdown",
          "string.other.link.description.title.markdown"
        ],
        "settings": {
          "foreground": "#AA00FF"
        }
      },
      {
        "name": "[MARKDOWN] - Link Address",
        "scope": [
          "markup.underline.link.markdown",
          "markup.underline.link.image.markdown"
        ],
        "settings": {
          "foreground": "#005f87"
        }
      },
      {
        "name": "[MARKDOWN] - Inline Code",
        "scope": ["fenced_code.block.language", "markup.inline.raw.markdown"],
        "settings": {
          "foreground": "#f00"
        }
      },
      {
        "name": "[MARKDOWN] - Code Block",
        "scope": ["fenced_code.block.language", "markup.inline.raw.markdown"],
        "settings": {
          "foreground": "#f00"
        }
      },
      {
        "name": "[TYPESCRIPT] - Entity Name Type",
        "scope": "source.ts entity.name.type",
        "settings": {
          "foreground": "#424242"
        }
      },
      {
        "name": "[TYPESCRIPT] - Keyword",
        "scope": "source.ts keyword",
        "settings": {
          "foreground": "#005f87",
          "fontStyle": "bold"
        }
      },

      // Hmmm....
      {
        "name": "Italicsify for Operator Mono",
        "scope": [
          "modifier",
          "this",
          "comment",
          "storage.modifier.js",
          "entity.other.attribute-name.js",
          "entity.other.attribute-name.html"
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },

      //
      // PYTHON Customizations.
      {
        "scope": [
          "keyword.operator.arithmetic.python",
          "keyword.operator.assignment",
          "keyword.operator.comparison.python",
          "keyword.operator.python"
        ],
        "settings": {
          "foreground": "#424242",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "keyword.control.import.python",
        "settings": {
          "foreground": "#d70000",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "entity.name.type.class.python",
        "settings": {
          "foreground": "#0087af",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "storage.modifier.declaration.python",
          "storage.type.class.python",
          "storage.type.function.python"
        ],
        "settings": {
          "foreground": "#d70087",
          "fontStyle": ""
        }
      },
      {
        "scope": [
          "entity.name.function.python",
          "support.function.magic.python"
        ],
        "settings": {
          "foreground": "#0087af",
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "keyword.control.flow.python",
          "keyword.operator.logical.python"
        ],
        "settings": {
          "foreground": "#8700af",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "meta.function-call.arguments.python",
        "settings": {
          "foreground": "#424242",
          "fontStyle": ""
        }
      },
      {
        "scope": "meta.function-call.generic.python",
        "settings": {
          "foreground": "#005f87",
          "fontStyle": ""
        }
      },
      {
        "scope": "keyword.codetag.notation.python",
        "settings": {
          "foreground": "#00af5f",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "support.type.exception.python",
        "settings": {
          "foreground": "#d70000",
          "fontStyle": ""
        }
      },
      {
        "scope": "meta.item-access.python",
        "settings": {
          "foreground": "#424242",
          "fontStyle": ""
        }
      },
      {
        "scope": [
            "support.function.builtin.python",
        ],
        "settings": {
